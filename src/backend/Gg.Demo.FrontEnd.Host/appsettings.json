{
  "Logging": {
    "LogLevel": {
      "Gd.Demo": "Debug",
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": { "EndpointDefaults": { "Protocols": "http2" } },
  "Jwt": {
    "Key": "your-secret-key-here-must-be-at-least-16-characters",
    "Issuer": "your-issuer",
    "Audience": "your-audience"
  },
  "Orleans": {
    "Clustering": {
      "ProviderType": "Redis",
      "Redis": {
        "ConnectionString": null
      }
    }
  }
}
