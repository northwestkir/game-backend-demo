{
  "Logging": {
    "LogLevel": {
      "Gg.Demo": "Debug",
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "System.Net.Http.HttpClient": "Warning"
    },
    "Console": {
      "FormatterName": "systemd",
      "FormatterOptions": { "TimestampFormat": "HH:mm:ss.fff | " }
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  }
}
