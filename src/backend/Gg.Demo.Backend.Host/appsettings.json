{
  "Logging": {
    "LogLevel": {
      "Gg.Demo": "Debug",
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "Console": {
      "FormatterName": "systemd",
      "FormatterOptions": { "TimestampFormat": "HH:mm:ss.fff | " }
    }
  },
  "AllowedHosts": "*",
  "OpenMatchFrontEnd": {
    "Address": null
  },
  "Orleans": {
    "ServiceId": "demo-game-backend",
    "ClusterId": null,
    "Clustering": {
      "ProviderType": "Redis",
      "Redis": {
        "ConnectionString": null
      }
    },
    "UseKubernetesHosting": true
  }
}
